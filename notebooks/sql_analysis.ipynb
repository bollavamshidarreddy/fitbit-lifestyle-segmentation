    "with engine.connect() as conn:\n",
    "    result = conn.execute(\n",
    "        text(\"\"\"\n",
    "            WITH TOTALS AS (SELECT \n",
    "                A.ID,\n",
    "                SUM(A.CALORIES) AS TOTAL_CALORIES_BURNT,\n",
    "                SUM(A.TOTALSTEPS) AS TOTAL_STEPS,\n",
    "                SUM(B.TotalTimeInBed) AS TOTAL_SLEEP\n",
    "            FROM dailyactivity_merged A\n",
    "            LEFT JOIN sleepday_merged B\n",
    "                ON A.ID = B.ID\n",
    "               AND A.ActivityDate = B.SleepDay\n",
    "            GROUP BY A.ID)\n",
    "            \n",
    "            SELECT ID, TOTAL_CALORIES_BURNT,RANK() OVER(ORDER BY TOTAL_CALORIES_BURNT DESC) AS CRK,\n",
    "            TOTAL_STEPS,\n",
    "            RANK() OVER(ORDER BY TOTAL_STEPS DESC) AS SRK,\n",
    "            TOTAL_SLEEP,\n",
    "            RANK() OVER(ORDER BY TOTAL_SLEEP DESC) AS SLEEP_RK\n",
    "            FROM TOTALS\n",
    "            ;\n",
    "        \"\"\")\n",
    "    )\n",
    "\n",
    "df = pd.DataFrame(result.fetchall(), columns=result.keys())\n",
    "df\n"
   ]
